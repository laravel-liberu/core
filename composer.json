        "laravel-liberu/sentry": "^1.0",
        "laravel-liberu/select": "^4.0",
        "laravel-liberu/track-who": "^2.0",
        "laravel-liberu/tables": "^4.0",
        "laravel-liberu/upgrade": "^2.5",
        "laravel-liberu/users": "^2.0",
        "voku/anti-xss": "^4.1"
